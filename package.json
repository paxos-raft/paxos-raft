{
    "name": "paxos-raft",
    "description": "ðŸ—£ Developer Automata: Exploitation of Developer's Intellectual Technical/Programming/Environment Sphere",
    "bin": "index.js",
    "author": "Prateek Rastogi",
    "homepage": "https://github.com/paxos-raft/paxos-raft/#readme",
    "license": "Xerox PARC",
    "version": "0.0.14",
    "publisher": "paxos-raft",
    "engines": {
        "vscode": "^1.42.0",
        "sketch": ">=49.0"
    },
    "categories": [
        "Extension Packs"
    ],
    "activationEvents": [
        "onCommand:extension.init",
        "workspaceContains:**/.paxos-raft-trigger-template-files"
    ],
    "main": "lib/vscode/vscode.js",
    "contributes": {
        "commands": [
            {
                "command": "extension.init",
                "title": "paxos-raft: Cast Automata Sphere"
            }
        ]
    },
    "scripts": {
        "lint": "standard packages/**/**/*.js --fix",
        "skpm:build": "skpm-build",
        "skpm:watch": "skpm-build --watch",
        "skpm:start": "skpm-build --watch --run",
        "skpm:postinstall": "npm run build && skpm-link",
        "lerna:bootstrap": "lerna bootstrap",
        "lerna:test": "lerna run test",
        "lerna:publish": "lerna version patch --yes && lerna publish from-package --yes"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lerna run lint",
            "pre-push": "lerna run lint"
        }
    },
    "devDependencies": {
        "husky": "^4.2.0",
        "lerna": "^3.20.2",
        "standard": "^14.3.1",
        "@skpm/builder": "^0.7.0"
    },
    "keywords": [
        "Sketch",
        "draw.io",
        "VS Code",
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "jupyterlab": {
        "extension": "lib/jupyterlab"
    },
    "skpm": {
        "name": "paxos-raft",
        "manifest": "lib/sketch/src/manifest.json",
        "main": "lib/sketch/paxos-raft.sketchplugin",
        "assets": [
            "lib/sketch/assets/**/*"
        ],
        "sketch-assets-file": "lib/sketch/sketch-assets/icons.sketch"
    }
}