{
    "name": "paxos-raft",
    "description": "npm NP Turing Machine Automata: Elucidations & Modulations of Developer Economics, Technical Docs, and Code Reuse",
    "bin": "index.js",
    "author": "Prateek Rastogi",
    "homepage": "https://github.com/paxos-raft/paxos-raft/#readme",
    "license": "ISC",
    "version": "0.0.10-alpha.30",
    "scripts": {
        "lint": "standard packages/**/**/*.js --fix",
        "lerna:bootstrap": "lerna bootstrap",
        "lerna:test": "lerna run test",
        "lerna:publish": "lerna version prerelease --yes && lerna publish from-package --yes"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lerna run lint",
            "pre-push": "lerna run lint"
        }
    },
    "devDependencies": {
        "husky": "^4.2.0",
        "lerna": "^3.20.2",
        "standard": "^14.3.1"
    },
    "keywords": [
        "Sketch",
        "drawio",
        "VS Code",
        "jupyter",
        "jupyterlab",
        "jupyterlab-extension"
    ],
    "jupyterlab": {
        "extension": "lib/jupyterlab"
    },
    "skpm": {
        "name": "paxos-raft",
        "manifest": "lib/sketch/manifest.json",
        "main": "paxos-raft.sketchplugin",
        "assets": [
            "lib/sketch/assets/**/*"
        ]
    }
}
